{
  "name": "@rotki/eslint-plugin",
  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "type": "module",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/rotki/eslint-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rotki/eslint-plugin.git"
  },
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf .nyc_output coverage dist docs/.vitepress/dist",
    "coverage": "nyc report --reporter lcov && opener coverage/lcov-report/index.html",
    "generate": "tsx scripts/update-rule-docs.ts",
    "generate:index": "tsx scripts/update-docs-index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "new": "tsx ./scripts/new-rule.ts",
    "docs": "vitepress dev docs",
    "docs:build": "pnpm run generate && pnpm run generate:index && vitepress build docs",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "release": "bumpp -r --no-push"
  },
  "peerDependencies": {
    "eslint": "^9.20.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "8.37.0",
    "debug": "4.4.1",
    "eslint-compat-utils": "0.6.5",
    "jsonc-eslint-parser": "2.4.0",
    "scule": "1.3.0",
    "vue-eslint-parser": "10.2.0",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@rotki/eslint-config": "4.1.0",
    "@types/debug": "4.1.12",
    "@types/node": "22.16.4",
    "@typescript-eslint/eslint-plugin": "8.37.0",
    "@typescript-eslint/parser": "8.37.0",
    "@typescript-eslint/rule-tester": "8.37.0",
    "@vitest/coverage-v8": "3.2.4",
    "bumpp": "10.2.0",
    "debug": "4.4.1",
    "eslint": "9.31.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "rimraf": "6.0.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "unbuild": "3.5.0",
    "vitepress": "1.6.3",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=22 <23",
    "pnpm": ">=10 <11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "lint-staged": {
    "*.{js,cjs,ts,vue,yml,json,md}": "eslint"
  }
}
